{
    "name": "",
    "version": 2,
    "alias": [
        "royugoapi"
    ],
    "env": {
        "go_mongo_db": "@go_mongo_db"
    },
    "builds": [
        {
            "src": "functions/*.go",
            "use": "@now/go@canary"
        },
        {
            "src": "public/index.html",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/api/createQuote",
            "dest": "/functions/createQuote.go",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/api/(.*)",
            "dest": "/functions/$1.go"
        },
        {
            "src": "/",
            "dest": "public/index.html"
        }
    ],
    "public": true
}