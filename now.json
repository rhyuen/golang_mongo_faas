{
    "name": "",
    "version": 2,
    "alias": [
        "royugoapi"
    ],
    "env": {
        "go_mongo_db": "@go_mongo_db"
    },
    "builds": [
        {
            "src": "functions/*.go",
            "use": "@now/go@canary"
        },
        {
            "src": "/public/**/*.*",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/api/quote",
            "dest": "/functions/createQuote.go",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/api/quote",
            "dest": "/functions/updateQuote.go",
            "methods": [
                "PUT"
            ]
        },
        {
            "src": "/api/quote",
            "dest": "/functions/deleteQuote.go",
            "methods": [
                "DELETE"
            ]
        },
        {
            "src": "/api/quote",
            "dest": "/functions/getQuotes.go",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/api/(.*)",
            "dest": "/functions/$1.go",
            "methods": [
                "GET"
            ]
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/",
            "dest": "public/index.html"
        }
    ],
    "public": true
}